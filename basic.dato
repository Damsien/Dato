PROGRAMME BASIQUE EST
I, SOMME, S, T1, T2, T3, T4, T5, T6, T7, T8, T9 : Entier
DEBUT
I <- 10
SOMME <- 0
T1 <- I > 0
T2 <- I = 0
T3 <- T1 OR T2
L1 IF T3 GOTO L3
GOTO L2
L3 SOMME <- SOMME + I
I <- I - 1
T4 <- I > 0
T5 <- I = 0
T6 <- T4 OR T5
T3 <- T6
GOTO L1
L2 NULL
T7 <- SOMME < 30
T8 <- SOMME = 30
T9 <- T7 OR T8
IF T9 GOTO L4
GOTO L6
L4 S <- 0
GOTO L5
L6 S <- 1
L5 NULL
FIN
